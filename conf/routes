#Error Routes
GET         /register-to-vote/error/timeout                              @uk.gov.gds.ier.controller.ErrorController.ordinaryTimeout
GET         /register-to-vote/error/forces/timeout                       @uk.gov.gds.ier.controller.ErrorController.forcesTimeout
GET         /register-to-vote/error/crown/timeout                        @uk.gov.gds.ier.controller.ErrorController.crownTimeout
GET         /register-to-vote/error/serverError                          @uk.gov.gds.ier.controller.ErrorController.serverError
GET         /register-to-vote/error/notFound                             @uk.gov.gds.ier.controller.ErrorController.notFound

# Map static resources from the /public folder to the /assets URL path
GET         /assets/messages.js                                          @uk.gov.gds.ier.langs.MessagesController.all
GET         /assets/:lang/messages.js                                    @uk.gov.gds.ier.langs.MessagesController.forLang(lang:String)
GET         /assets/template/*file                                       @uk.gov.gds.ier.assets.Template.at(path="/app/assets/mustache/govuk_template_inheritance/assets", file)
GET         /assets/toolkit/*file                                        @uk.gov.gds.ier.assets.GovukToolkit.at(path="/app/assets/govuk_frontend_toolkit", file)
GET         /assets/*file                                                @uk.gov.gds.ier.assets.Assets.at(path="/public", file)

# Status route
GET         /status                                                      @uk.gov.gds.ier.controller.StatusController.status



